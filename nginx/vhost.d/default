#/* vim: set ft=cfg : */

# Remove trailing slashes
# http://nginx.org/en/docs/http/ngx_http_rewrite_module.html#rewrite
rewrite ^/(.*)/$ /$1 permanent;

# Disable response buffering to temporary files
# http://nginx.org/en/docs/http/ngx_http_proxy_module.html#proxy_max_temp_file_size
proxy_max_temp_file_size 0;

# Sets the maximum allowed size of the client request body,
# specified in the “Content-Length” request header field
# http://nginx.org/en/docs/http/ngx_http_core_module.html#client_max_body_size
client_max_body_size 200M;

location ^~ /uploads {
  root /var/www;

  # Set max expires for all these file types
  location ~ \.(?:gif|jpe?g|png|svg)$ {
    add_header Cache-Control 'public';
    expires max;
  }

  location ~ \.mp3$ {
    # fast.ly caching configuration
    # https://docs.fastly.com/guides/tutorials/cache-control-tutorial.html
    add_header Surrogate-Control 'max-age=3600, stale-if-error=86400';
    keepalive_timeout 60; # global is 30
    send_timeout 60; # global is 10

    open_file_cache max=10000 inactive=30s;
    open_file_cache_errors on;
  }
}

# Legacy static assets, baked into the container since they never change
location ^~ /wp-content {
  root /var/www;
  add_header Cache-Control 'public';
  expires max;
}

# Return success with no content for some legacy resources
location ^~ /reactpodcast/wp-content { return 204; }
location ~ wp-login\.php { return 204; }

# Sponsor redirects
location /datadog { return 301 $scheme://www.datadoghq.com/lpgs/?utm_source=Advertisement&utm_medium=Advertisement&utm_campaign=ChangelogPodcast-Tshirt; }
location /sentry { return 301 $scheme://sentry.io/from/changelog/; }

# Relatively recent resources that have permanently changed location
location ~ ^/([0-9]+) { return 301 $scheme://$server_name/podcast/$1; }
location ~ ^/gotime-([0-9]+) { return 301 $scheme://$server_name/gotime/$1; }
location ~ ^/rfc-([0-9]+) { return 301 $scheme://$server_name/rfc/$1; }
location ~ ^/channel/(.+)$ { return 301 $scheme://$server_name/topic/$1; }

location /rss { return 301 $scheme://$server_name/feed; }
location /podcast/rss { return 301 $scheme://$server_name/podcast/feed; }
location /team { return 301 $scheme://$server_name/about; }
location /changeloggers { return 301 $scheme://$server_name/about; }
location /membership { return 301 $scheme://$server_name/community; }
location /store { return 301 $scheme://$server_name/community; }
location /sponsorship { return 301 $scheme://$server_name/sponsor; }
location /soundcheck { return 301 $scheme://$server_name/guest; }
location /submit { return 301 $scheme://$server_name/news/submit; }

location /podcast/archive { return 301 $scheme://$server_name/podcast; }
location /founderstalk/archive { return 301 $scheme://$server_name/founderstalk; }
location /gotime/archive { return 301 $scheme://$server_name/gotime; }
location /rfc/archive { return 301 $scheme://$server_name/rfc; }
location /spotlight/archive { return 301 $scheme://$server_name/spotlight; }
location /jsparty/archive { return 301 $scheme://$server_name/jsparty; }
location /master/archive { return 301 $scheme://$server_name/master; }

location /post/980148470/episode-0-3-2-960-gs-with-nathan-smith { return 301 $scheme://$server_name/podcast/32; }

# Legacy resources that have permanently changed location
